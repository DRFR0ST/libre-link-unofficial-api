{
  "name": "libre-link-unofficial-api",
  "module": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "version": "1.0.0-alpha.2",
  "description": "An unofficial API for Libre Link Up (glucose monitoring system/CGM)",
  "author": "Mike Eling <mike.eling97@gmail.com>",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "bun run build:node && bun run build:emitDeclarations",
    "build:node": "bun build ./src/index.ts --outdir dist --target node --splitting --minify --sourcemap=inline",
    "build:emitDeclarations": "tsc --emitDeclarationOnly --project tsconfig.json --tsBuildInfoFile './dist/.tsbuildinfo'",
    "clean": "rimraf dist",
    "test": "bun run test-unit && bun run test-integrity",
    "test-unit": "bun test 'spec' --coverage",
    "test-integrity": "bun test 'int' --coverage --timeout 60000",
    "test-integrity:update": "bun test 'int' --coverage --update-snapshots --timeout 60000",
    "example": "bun run ./example/index.ts",
    "prepublish": "bun run build"
  },
  "devDependencies": {
    "bun-types": "latest",
    "rimraf": "latest",
    "nock": "^13.5.4"
  },
  "peerDependencies": {
    "typescript": "latest"
  },
  "files": [
    "dist/*.js",
    "dist/*.d.ts"
  ]
}